                    ull = (unsigned long)ld;
                    memcpy(s2, &ull, 8);

                    /* The unsigned long is the same as signed long. */
                    if(s2[0]==0x00 && s2[1]==0x47 && s2[2]==0x33 && s2[3]==0xce &&
                       s2[4]==0x17 && s2[5]==0xaf && s2[6]==0x22 && s2[7]==0x7f)
